<h1>Listing google_sheets</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Source</th>
  </tr>

<% @sheets.each do |google_sheet| %>
  <tr>
    <td><%= google_sheet["title"] %></td>
    <td><%= google_sheet["src"] %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Google sheet', new_google_sheet_path %>
